{
  "name": "evolvevoicesvcs-ivshierarchy",
  "version": "0.0.14",
  "description": "Node.js on Openshift",
  "cmdbAppID": 2817,
  "artifactId": "evolvevoicesvcs-ivshierarchy",
  "groupId": "telus.oe-b2b.evolvevoicesvcs-ivshierarchy",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write src/",
    "test": "node ./postman/tests.js",
    "start": "node --unhandled-rejections=warn dist/app.js",
    "dev": "set NODE_EXTRA_CA_CERTS=cert/cso-tcso-ca.pem && nodemon src/app.ts",
    "debug": "set NODE_EXTRA_CA_CERTS=cert/cso-tcso-ca.pem && nodemon --inspect=5858 -e ts,tsx --exec node -r ts-node/register src/app.ts",
    "build": "tsc -p .",
    "postbuild": "cpy --cwd=src --parents '**/*.json' '../dist/'",
    "prepare": "husky install",
    "sonar": "sonar-scanner"
  },
  "scriptsComments": {
    "UV_THREADPOOL_SIZE_NOT_NEEDED": "maxPoolSize + 4",
    "NODE_EXTRA_CA_CERTS": "existings openshift env variables - so not needed in start, if you want to use it for dev , add set NODE_EXTRA_CA_CERTS",
    "startWindows": "set UV_THREADPOOL_SIZE=8 && node dist/app.js",
    "startLinux": "export UV_THREADPOOL_SIZE=8 && node dist/app.js",
    "dev_without_certificate": "set UV_THREADPOOL_SIZE=8 && nodemon src/app.ts",
    "dev": "set NODE_EXTRA_CA_CERTS=cert/cso-tcso-ca.pem && nodemon src/app.ts",
    "devLinux": "export UV_THREADPOOL_SIZE=8 && nodemon src/app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
