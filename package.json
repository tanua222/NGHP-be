    "@types/oracledb": "^4.2.0",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.4",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "class-transformer": "^0.4.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "csv-writer": "^1.6.0",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.1",
    "express-session": "^1.17.1",
    "json-transformer-node": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.4",
    "lodash": "^4.17.15",
